#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

echo "Installing csh..."
# Install csh
sudo yum -y install csh >> $DIR/../../sitc.log 2>&1

echo "Installing gcc-gfortran..."
# Install gcc-gfortran
sudo yum -y install gcc-gfortran >> $DIR/../../sitc.log 2>&1

$DIR/../common/install_gamess
